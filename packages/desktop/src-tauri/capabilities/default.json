{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:path:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "notification:default",
    "notification:allow-request-permission",
    "notification:allow-notify",
    "notification:allow-is-permission-granted",
    "autostart:default",
    "autostart:allow-enable",
    "autostart:allow-disable",
    "autostart:allow-is-enabled",
    "shell:default",
    "shell:allow-open",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-write-text-file",
    "fs:allow-write-file",
    "fs:scope-app",
    "fs:scope-appdata",
    "fs:scope-applocaldata",
    "fs:scope-download",
    "fs:scope-document",
    "fs:scope-desktop",
    "dialog:default",
    "dialog:allow-save",
    "http:default",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "log:default",
    "log:allow-log",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install"
  ]
}
